<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="PhotoBoard">
  <insert id="selectMemberList" parameterType="com.file.board.vo.PhotoBoardVO">
   insert into photo_board(pb_num,pb_title,pb_content,pb_photo_name,pb_photo_path,TO_CHAR(SYSDATE, 'YYYYMMDD'),TO_CHAR(SYSDATE,'HH24MISS'))
   value(seq_pb_num.nextval,#{pb_title},#{pb_content},#{pb_photo_name},#{pb_photo_path},TO_CHAR(SYSDATE, 'YYYYMMDD'),TO_CHAR(SYSDATE,'HH24MISS'))
  </insert>
  
</mapper>